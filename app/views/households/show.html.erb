<div class="page-header">
  <%= link_to households_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_household_path(@household), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show household</h1>
</div>

<dl class="dl-horizontal">

  <dt>Name:</dt>
  <dd><%= @household.name %></dd>


  <dt>Address:</dt>
  <dd><%= @household.address %></dd>


  <dt>City:</dt>
  <dd><%= @household.city %></dd>


  <dt>State:</dt>
  <dd><%= @household.state %></dd>


  <dt>Country:</dt>
  <dd><%= @household.country %></dd>


  <dt>Phone:</dt>
  <dd><%= @household.phone %></dd>


</dl>
