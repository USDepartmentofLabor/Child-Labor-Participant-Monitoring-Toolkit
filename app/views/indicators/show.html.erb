<div class="page-header">
  <%= link_to indicators_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_indicator_path(@indicator), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show indicator</h1>
</div>

<dl class="dl-horizontal">

  <dt>Code:</dt>
  <dd><%= @indicator.code %></dd>


  <dt>Desc:</dt>
  <dd><%= @indicator.desc %></dd>


  <dt>Indicator type:</dt>
  <dd><%= @indicator.indicator_type %></dd>


  <dt>String:</dt>
  <dd><%= @indicator.string %></dd>


  <dt>User:</dt>
  <dd><%= @indicator.user_id %></dd>


  <dt>Project:</dt>
  <dd><%= @indicator.project %></dd>


</dl>
