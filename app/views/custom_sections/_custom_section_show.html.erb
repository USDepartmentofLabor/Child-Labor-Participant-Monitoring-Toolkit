<div class="box box-primary">
  <div class="box-header with-border">
    <%= fa_icon "info" %>
    <h3 class="box-title">
      <%= custom_section_show.title %>
    </h3>
  </div>
  <div class="box-body no-padding">
    <dl>
      <% custom_section_show.custom_fields.each do |custom_field| %>
        <dt><%= custom_field.name %></dt>
        <dd><%= custom_field_value(custom_field) %></dd>
      <% end %>
    </dl>
  </div>
</div>
