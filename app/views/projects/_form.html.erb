<%= form_for(@project, html: { role: "form" }) do |f| %>
  <%= error_messages(@project) %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <button type="submit" class="btn btn-primary"><%= t("general.submit") %></button>
  </div>
<% end %>