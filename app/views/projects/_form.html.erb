<%= form_for(@project, html: { role: "form" }) do |f| %>
  <%= error_messages(@project) %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :cop_num %>
    <%= f.text_field :cop_num, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :start_date %>
    <div class="selection">
      <%= f.date_select :start_date, {}, class: "form-control select-inline" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :end_date %>
    <div class="selection">
      <%= f.date_select :end_date, {}, class: "form-control select-inline" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :org %>
    <%= f.text_field :org, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :proj_type %>
    <%= f.select :proj_type, ["CLEP", "IPEC"], {include_blank: true}, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :funding %>
    <%= f.text_field :funding, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :country %>
    <%= f.country_select :country, {include_blank: true}, {class: "form-control"} %>
  </div>
  <div class="form-group">
    <%= f.label :state %>
    <%= text_field_tag "projects[state]", nil, {include_blank: true, class: "form-control"} %>
  </div>
  <div class="form-group">
    <button type="submit" class="btn btn-primary"><%= t("general.submit") %></button>
  </div>
<% end %>