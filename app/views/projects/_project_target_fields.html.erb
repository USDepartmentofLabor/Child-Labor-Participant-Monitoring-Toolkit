<fieldset class="asscciated-fields">
  <div class="form-group">
    <%= f.label :project_project_target_type_id %>
    <%= f.select :project_project_target_type_id,
                 ProjectTargetType.all.collect { |t| [t.name, t.id] }.sort,
                 { include_blank: true },
                 class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :project_target %>
    <%= f.text_field :project_target, class: "form-control" %>
  </div>

  <%= f.hidden_field :_destroy %>
  <%= link_to '#', class: "remove_fields pull-right" do %>
    <i class="fa fa-trash-o"></i> <%= t("general.delete") %>
  <% end %>
</fieldset>
