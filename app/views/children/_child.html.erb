<tr id='<%= "child-#{child.id}" %>'>
<td><%= link_to child.id, child_path(child) %></td>
<td><%= child.first_name %></td>
<td><%= child.last_name %></td>
<td><%= child.gender_name %></td>
<td><%= child.age %></td>
<td><%= child.intake_date %></td>
<td><%= child.current_education_status.present? ? child.current_education_status.name : "" %></td>
<td><%= child.current_work_status.present? ? child.current_work_status.name : "" %></td>
</tr>
