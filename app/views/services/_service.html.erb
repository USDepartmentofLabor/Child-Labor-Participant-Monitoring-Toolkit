<tr>
<td><%= service.id %></td>
<td><%= service.name %></td>
<td><%= service.service_type.name unless service.service_type.nil? %></td>
<td><%= service.start_date %></td>
<td><%= service.end_date %></td>
<td><%= link_to t("general.show"), project_service_path(@project, service) %></td>
<td><%= link_to t("general.edit"), edit_project_service_path(@project, service) %></td>
<td><%= link_to t("general.delete"), project_service_path(@project, service), method: :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>
