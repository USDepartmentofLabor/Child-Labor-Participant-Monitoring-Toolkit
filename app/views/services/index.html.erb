<div class="page-header">
  <%= link_to new_project_service_path(@project), class: 'btn btn-primary' do %>
    <%= fa_icon "plus" %>
    <%= t("services.index.add") %>
  <% end %>
  <h1><%= t("services.index.title") %></h1>
</div>

<div class="panel panel-default">
  <div class="panel-body">
    <table class="table table-condensed table-striped" id="child-data-table">
      <thead>
        <tr>
          <th>ID</th>
          <th><%= Service.human_attribute_name(:name) %></th>
          <th><%= Service.human_attribute_name(:service_type) %></th>
          <th><%= Service.human_attribute_name(:start_date) %></th>
          <th><%= Service.human_attribute_name(:end_date) %></th>
          <th></th>
          <th></th>
          <th></th>
        </tr>
      </thead>

      <tbody>
        <%= render(partial: "service", collection: @services) %>
      </tbody>
    </table>
  </div>
</div>
